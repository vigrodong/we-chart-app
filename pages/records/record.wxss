.records{
  height: 100%;
    width:100%;
    display:flex;
    flex-wrap:wrap;
    justify-content:center;
    padding-top: 30px;
    font-family:Arial,Verdana,Sans-serif;
    font-size:14px;
} 
.record{
  width:90%;
  border-radius: 10px;
  height:100px;
  margin: 3px 0;
  padding:3%;
  overflow: hidden;
}
.pending{
  background-color: #FFCE44;
}
.success{
  background-color: #7CD238;
}
.failed{
  background-color: #EDECE8;
}
